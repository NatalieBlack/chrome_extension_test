{
    "manifest_version": 2,
    "name": "My Cool Extension",
    "version": "0.1",

    "content_scripts": [
    {
        "matches": [
            "<all_urls>"
            ],
        "js": ["content.js"]
    }
    ],
    "browser_action": {
      "default_icon": "icon.png"
    },
    "permissions": [
        "http://localhost:3000/home.json"
              ],
    "background": {
      "scripts": ["background.js"]
    }
}
